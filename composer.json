{
    "name": "laravel-notification-channels/at-sms-notify",
    "description": "Send Laravel Notifications Via Africa's Talking API",
    "homepage": "https://github.com/laravel-notification-channels/at-sms-notify",
    "license": "MIT",
    "authors": [
        {
            "name": "Osen Concepts",
            "email": "hi@osen.co.ke",
            "homepage": "https://osen.co.ke",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/notifications": "~5.5 || ~6.0",
        "illuminate/support": "~5.5 || ~6.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\AT\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\AT\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test:coverage": "phpunit --coverage-text --coverage-clover=coverage.clover"
    },
    "config": {
        "sort-packages": true
    }
}
